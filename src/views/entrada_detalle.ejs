<%- include('partials/_header')%>
</header>
<div class="container-fluid mb-3">
    <div class="row justify-content-center">
        <div class="col col-lg-10">
            <table class="table table-bordered table-hover text-center" style="background-color: white;">
                <div class="text-center text-white bg-primary h5">
                    <div class="text-center text-white bg-primary h5">
                            <h4>Lista de entradas</h4>
                    </div>
                </div>
                <thead>
                    <tr>
                        <th>identificador de detalle</th>
                        <th>materia prima </th>
                        <th>cantidad de entrada</th>
                    </tr>
                </thead>
                <tbody>
                    <%if (data){%>
                        <% for (var i = 0; i < data.length; i++){%>
                        <tr>
                            <td><%=data[i].id_detalle_entrada%></td>
                            <td><%=data[i].nombre_materia_prima%></td>
                            <td><%=data[i].cantidad_entrada%></td>
                        </tr>
                        <% }%>
                    <%}%>
                </tbody>
            </table>
        </div>
    </div>
</div>
<%- include('partials/_footer')%>